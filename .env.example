# 服务配置
PORT=3070
REGISTER_BASE_URL=https://register.example.com        # 可以不填，默认就是IP加端口
SESSION_SECRET=your-random-secret-key-change-this-in-production

# 管理员面板配置（可选）
ADMIN_PANEL_PASSWORD=admin123
REQUIRE_INVITE_CODE=false
ADMIN_LOGIN_PATH=/admin/login
ADMIN_PANEL_PATH=/admin
MAX_LOGIN_ATTEMPTS=5
LOGIN_LOCKOUT_TIME=15

# GitHub OAuth 配置（可选）
ENABLE_GITHUB_OAUTH=false
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Discord OAuth 配置（可选）
ENABLE_DISCORD_OAUTH=false
DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret

# Linux.do OAuth 配置（可选）
ENABLE_LINUXDO_OAUTH=false
LINUXDO_CLIENT_ID=your_linuxdo_client_id
LINUXDO_CLIENT_SECRET=your_linuxdo_client_secret
